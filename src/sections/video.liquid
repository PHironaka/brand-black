{% schema %}
  {
    "name": "Hero Video",
    "class": "hidden-xs",
    "settings": [
		{
			"type":	"select",
			"id": 	"video_type",
			"label": "Video Source",
			"options": [
				{ 
                  "group": "youtube",
                  "value": "youtube", 
                  "label": "YouTube"  
                },
				{ 
                  "group": "vimeo",
                  "value": "vimeo", 
                  "label": "Vimeo" 
                }
			],
			"default": "vimeo",
            "info": "From which site is the video streaming from?"
		},
		{
			"type": "text",
			"id": "video_id",
			"label": "Video ID",
			"default": "_9VUPq3SxOc",
			"info": "[Find your YouTube video ID](https://docs.joeworkman.net/rapidweaver/stacks/youtube/video-id)."
		}
    ],
    "presets": [
      {
        "name": "Video",
        "category": "Frontpage Sections"
      }
    ]
  }
{% endschema %}
